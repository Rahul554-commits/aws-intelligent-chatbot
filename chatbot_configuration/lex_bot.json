{
  "name": "StudentHelpBot",
  "locale": "en-US",
  "childDirected": false,
  "intents": [
    {
      "intentName": "GetCourseInfo",
      "intentVersion": "$LATEST"
    }
  ],
  "clarificationPrompt": {
    "maxAttempts": 2,
    "messages": [
      {
        "contentType": "PlainText",
        "content": "I didn't understand that. Can you rephrase?"
      }
    ]
  },
  "abortStatement": {
    "messages": [
      {
        "contentType": "PlainText",
        "content": "Sorry, I'm not able to help with that."
      }
    ]
  },
  "voiceId": "Joanna"
}
